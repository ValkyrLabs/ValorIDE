    "husky": "^9.1.7",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.3.3",
    "should": "^13.2.3",
    "typescript": "^5.4.5"
  },
  "files": [
    "dist/",
    "assets/",
    "package.json",
    "README.md",
    "LICENSE"
