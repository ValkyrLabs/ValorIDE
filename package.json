{
  "name": "valoride-dev",
  "displayName": "ValorIDE",
  "description": "Agentic coding right in your IDE, capable of creating/editing files, running commands, using the browser, and more with your permission every step of the way.",
  "version": "3.20.155",
  "icon": "assets/icons/icon.png",
  "galleryBanner": {
    "color": "#617A91",
    "theme": "dark"
  },
  "engines": {
    "vscode": "^1.84.0"
  },
  "author": {
    "name": "Valkyr Labs Inc"
  },
  "license": "Valkyr Labs Inc (https://valkyrlabs.com) 2.0",
  "publisher": "ValkyrLabsInc",
  "repository": {
    "type": "git",
    "url": "https://github.com/valkyrlabs/valor-ide"
  },
  "homepage": "https://valkyrlabs.com",
  "categories": [
    "AI",
    "Chat",
    "Programming Languages",
    "Education",
    "Snippets",
    "Testing"
  ],
  "keywords": [
    "valoride",
    "claude",
    "dev",
    "mcp",
    "openrouter",
    "coding",
    "agent",
    "autonomous",
    "chatgpt",
    "sonnet",
    "opus",
    "ai",
    "llama"
  ],
  "activationEvents": [
    "onStartupFinished",
    "onView:valoride-dev.SidebarProvider",
    "onViewContainer:valoride-activitybar",
    "onCommand:valoride.openInNewTab",
    "onCommand:valoride.diagnostics",
    "onCommand:valoride.fixLayout"
  ],
  "main": "./dist/extension.js",
  "extensionKind": [
    "ui",
    "workspace"
  ],
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "valoride-activitybar",
          "title": "Valor IDE",
          "icon": "assets/icons/icon.svg"
        }
      ]
    },
    "views": {
      "valoride-activitybar": [
        {
          "type": "webview",
          "id": "valoride-dev.SidebarProvider",
          "name": "ValorIDE"
        }
      ]
    },
    "commands": [
      {
        "command": "valoride.plusButtonClicked",
        "title": "New Task",
        "icon": "$(add)"
      },
      {
        "command": "valoride.accountButtonClicked",
        "title": "Account Settings",
        "icon": "$(zap)"
      },
      {
        "command": "valoride.mcpButtonClicked",
        "title": "MCP Servers",
        "icon": "$(server)"
      },
      {
        "command": "valoride.historyButtonClicked",
        "title": "History",
        "icon": "$(history)"
      },
      {
        "command": "valoride.popoutButtonClicked",
        "title": "Open in Editor",
        "icon": "$(link-external)"
      },
      {
        "command": "valoride.settingsButtonClicked",
        "title": "Settings",
        "icon": "$(settings-gear)"
      },
      {
        "command": "valoride.openInNewTab",
        "title": "Open In New Tab",
        "category": "ValorIDE"
      },
      {
        "command": "valoride.diagnostics",
        "title": "Show Diagnostics",
        "category": "ValorIDE"
      },
      {
        "command": "valoride.fixLayout",
        "title": "Fix Layout (Reset View Locations)",
        "category": "ValorIDE"
      }
    ],
    "menus": {
      "view/title": [
        {
          "command": "valoride.plusButtonClicked",
          "group": "navigation@1",
          "when": "view == valoride-dev.SidebarProvider"
        },
        {
          "command": "valoride.mcpButtonClicked",
          "group": "navigation@2",
          "when": "view == valoride-dev.SidebarProvider"
        },
        {
          "command": "valoride.historyButtonClicked",
          "group": "navigation@3",
          "when": "view == valoride-dev.SidebarProvider"
        },
        {
          "command": "valoride.popoutButtonClicked",
          "group": "navigation@4",
          "when": "view == valoride-dev.SidebarProvider"
        },
        {
          "command": "valoride.settingsButtonClicked",
          "group": "navigation@5",
          "when": "view == valoride-dev.SidebarProvider"
        },
        {
          "command": "valoride.accountButtonClicked",
          "group": "navigation@6",
          "when": "view == valoride-dev.SidebarProvider"
        },
        {
          "command": "valoride.fixLayout",
          "group": "navigation@7",
          "when": "view == valoride-dev.SidebarProvider"
        }
      ],
      "editor/title": [
        {
          "command": "valoride.plusButtonClicked",
          "group": "navigation@1",
          "when": "valoride.isValorIDETab"
        },
        {
          "command": "valoride.mcpButtonClicked",
          "group": "navigation@2",
          "when": "valoride.isValorIDETab"
        },
        {
          "command": "valoride.historyButtonClicked",
          "group": "navigation@3",
          "when": "valoride.isValorIDETab"
        },
        {
          "command": "valoride.settingsButtonClicked",
          "group": "navigation@4",
          "when": "valoride.isValorIDETab"
        },
        {
          "command": "valoride.accountButtonClicked",
          "group": "navigation@5",
          "when": "valoride.isValorIDETab"
        }
      ]
    },
    "configuration": {
      "title": "ValorIDE",
      "properties": {
        "valoride.vsCodeLmModelSelector": {
          "type": "object",
          "properties": {
            "vendor": {
              "type": "string",
              "description": "The vendor of the language model (e.g. copilot)"
            },
            "family": {
              "type": "string",
              "description": "The family of the language model (e.g. gpt-4)"
            }
          },
          "description": "Settings for VSCode Language Model API"
        },
        "valoride.mcp.mode": {
          "type": "string",
          "enum": [
            "full",
            "server-use-only",
            "off"
          ],
          "enumDescriptions": [
            "Enable all MCP functionality (server use and build instructions)",
            "Enable MCP server use only (excludes instructions about building MCP servers)",
            "Disable all MCP functionality"
          ],
          "default": "full",
          "description": "Controls MCP inclusion in prompts, reduces token usage if you only need access to certain functionality."
        },
        "valoride.enableCheckpoints": {
          "type": "boolean",
          "default": true,
          "description": "Enables extension to save checkpoints of workspace throughout the task."
        },
        "valoride.disableBrowserTool": {
          "type": "boolean",
          "default": false,
          "description": "Disables extension from spawning browser session."
        },
        "valoride.modelSettings.o3Mini.reasoningEffort": {
          "type": "string",
          "enum": [
            "low",
            "medium",
            "high"
          ],
          "default": "medium",
          "description": "Controls the reasoning effort when using the o3-mini model. Higher values may result in more thorough but slower responses."
        },
        "valoride.chromeExecutablePath": {
          "type": "string",
          "default": null,
          "description": "Path to Chrome executable for browser use functionality. If not set, the extension will attempt to find or download it automatically."
        }
      }
    }
  },
  "scripts": {
    "package:staging": "yarn run build:webview:staging && yarn run check-types && yarn run lint && node esbuild.js --production",
    "package:prod": "yarn run build:webview:prod && yarn run check-types && yarn run lint && node esbuild.js --production",
    "v": "node increment-version.js && vsce package",
    "vscode:prepublish": "yarn run package",
    "compile": "yarn run check-types && yarn run lint && node esbuild.js",
    "watch": "npm-run-all -p watch:*",
    "watch:esbuild": "node esbuild.js --watch",
    "watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
    "package": "yarn run build:webview && yarn run check-types && yarn run lint && node esbuild.js --production",
    "compile-tests": "tsc -p ./tsconfig.test.json --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "yarn run compile-tests && yarn run compile && yarn run lint",
    "check-types": "tsc --noEmit",
    "lint": "eslint src",
    "format": "prettier . --check",
    "format:fix": "prettier . --write",
    "test": "vscode-test",
    "install:all": "yarn install && cd webview-ui && yarn install",
    "start:webview": "cd webview-ui && yarn run start",
    "build:webview": "cd webview-ui && yarn run build",
    "test:webview": "cd webview-ui && yarn run test",
    "publish:marketplace": "vsce publish && ovsx publish",
    "publish:marketplace:prerelease": "vsce publish --pre-release && ovsx publish --pre-release",
    "prepare": "husky",
    "changeset": "changeset",
    "version-packages": "changeset version"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.12",
    "@testing-library/react": "^16.3.0",
    "@types/adm-zip": "^0.5.7",
    "@types/chai": "^5.0.1",
    "@types/diff": "^5.2.1",
    "@types/jest": "^29.5.14",
    "@types/mocha": "^10.0.7",
    "@types/node": "20.x",
    "@types/should": "^11.2.0",
    "@types/vscode": "^1.84.0",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "chai": "^4.3.10",
    "esbuild": "^0.25.0",
